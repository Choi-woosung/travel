<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
			PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="sSQL"> 
	
	<!-- 스케쥴리스트가져오기 -->
	<select id="scheduleList" resultType="sVO" parameterType="String">
		SELECT
			s_no sNo, s_name sName
		FROM
			schedule, member
		WHERE
			(SELECT m_no FROM member WHERE m_id = #{SID}) = 1
	</select>
	
</mapper>