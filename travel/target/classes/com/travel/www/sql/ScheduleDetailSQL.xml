<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
			PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="diningSQL"> 
	
	<insert id="makeDetail" parameterType="detailVO">
		INSERT INTO
			scheduledetail
		VALUES(
			#{sdno}, #{sdday}, #{sdono}, #{type},
			#{sdotype}, #{sdbody}
		)
	</insert>
	
</mapper>