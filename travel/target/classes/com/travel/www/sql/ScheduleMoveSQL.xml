<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
			PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="moveSQL"> 
	
	<insert id="makeMove" parameterType="moveVO">
		INSERT INTO
			schedulemove
		VALUES(
			#{mno}, #{mtype}, #{mname}, #{msloc},
			#{meloc}, #{rcost}, #{type}
		)
	</insert>
	
</mapper>