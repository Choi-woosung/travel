<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
			PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="restSQL"> 
	
	<insert id="makeRest" parameterType="restVO">
		INSERT INTO
			schedulerest
		VALUES(
			#{rno}, #{rname}, #{raddress}, #{rsdate},
			#{redate}, #{rcost}, #{type}
		)
	</insert>
	
</mapper>